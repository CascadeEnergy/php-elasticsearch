{
  "name": "cascade-energy/php-elasticsearch",
  "description": "Support classes for ElasticSearch in PHP",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Berube",
      "email": "charles.berube@cascadeenergy.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "CascadeEnergy\\ElasticSearch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CascadeEnergy\\Tests\\ElasticSearch\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/cberube/php-symfony-event-dispatcher.git"
    }
  ],
  "require": {
    "CascadeEnergy/php-symfony-event-dispatcher": "0.1.x-dev",
    "elasticsearch/elasticsearch": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7"
  }
}
